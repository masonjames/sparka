{
  "name": "chatjs",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "bun run check-env",
    "dev": "bun run check-env && bun run skiller:apply && bash scripts/with-db.sh next dev",
    "dev:inspect": "bun run skiller:apply && bash scripts/with-db.sh next dev --inspect",
    "build": "tsx lib/db/migrate && next build",
    "analyze": "next experimental-analyze",
    "start": "next start",
    "prod": "bun run build && bun run start",
    "lint": "bunx ultracite check",
    "format": "bunx ultracite fix",
    "storybook": "storybook dev -p 6006",
    "check-env": "bun scripts/check-env.ts",
    "build:storybook": "storybook build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bash scripts/with-db.sh bunx tsx lib/db/migrate.ts",
    "db:backfill-parts": "npx tsx lib/db/backfill-parts.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:branch:use": "bash scripts/db-branch-use.sh",
    "db:branch:create": "bash scripts/db-branch-create.sh",
    "db:branch:delete": "bash scripts/db-branch-delete.sh",
    "db:branch:start": "bash -c 'N=${1:-dev-local}; bun db:branch:create $N && bun db:branch:use $N' --",
    "db:branch:stop": "bash -c 'N=${1:-dev-local}; bun db:branch:use main && bun db:branch:delete $N' --",
    "db:branch:list": "bunx neonctl branches list",
    "eval:dev": "evalite watch",
    "eval:serve": "evalite serve",
    "skiller:apply": "bunx skiller@latest apply",
    "test": "bun test:playwright && bun test:unit",
    "test:playwright": "export PLAYWRIGHT=True && bunx playwright test --workers=4",
    "test:unit": "vitest run",
    "test:types": "rm -rf .next/dev/types && next typegen . && tsc --noEmit",
    "ai:devtools": "bunx @ai-sdk/devtools",
    "hooks:enable": "husky",
    "hooks:disable": "git config core.hooksPath .git/hooks",
    "fetch:models": "bun scripts/fetch-models.ts && bun format"
  },
