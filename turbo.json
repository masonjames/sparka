{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DATABASE_URL",
    "AUTH_SECRET",
    "BLOB_READ_WRITE_TOKEN",
    "AUTH_GOOGLE_ID",
    "AUTH_GOOGLE_SECRET",
    "AUTH_GITHUB_ID",
    "AUTH_GITHUB_SECRET",
    "VERCEL_APP_CLIENT_ID",
    "VERCEL_APP_CLIENT_SECRET",
    "AI_GATEWAY_API_KEY",
    "VERCEL_OIDC_TOKEN",
    "OPENROUTER_API_KEY",
    "OPENAI_COMPATIBLE_BASE_URL",
    "OPENAI_COMPATIBLE_API_KEY",
    "OPENAI_API_KEY",
    "CRON_SECRET",
    "REDIS_URL",
    "TAVILY_API_KEY",
    "EXA_API_KEY",
    "FIRECRAWL_API_KEY",
    "MCP_ENCRYPTION_KEY",
    "VERCEL_TEAM_ID",
    "VERCEL_PROJECT_ID",
    "VERCEL_TOKEN",
    "VERCEL_SANDBOX_RUNTIME",
    "APP_URL",
    "VERCEL_URL"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "format": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["playwright-report/**", "test-results/**"]
    },
    "test:unit": {
      "dependsOn": ["^test:unit"]
    },
    "test:types": {
      "dependsOn": ["^test:types"]
    },
    "docs:links": {
      "cache": false
    },
    "fetch:models": {
      "cache": false
    },
    "test:e2e": {
      "cache": false
    },
    "skiller:apply": {
      "cache": false
    }
  }
}
